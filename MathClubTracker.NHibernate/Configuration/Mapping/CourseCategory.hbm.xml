<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping namespace="MathClubTracker.Domain.DomainObjects" assembly="MathClubTracker.Domain" xmlns="urn:nhibernate-mapping-2.2">
<class name="CourseCategory" table="CourseCategory" >
    <id name="Id" type="Int32">
        <column name="Id"  />
        <generator class="assigned" />
    </id>

    <property name="CategoryName" type="AnsiString">
      <column name="CategoryName" not-null="true" length="50" sql-type="varchar" />
    </property>

 </class>
</hibernate-mapping>
       






