<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping namespace="MathClubTracker.Domain.DomainObjects" assembly="MathClubTracker.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ClassSessionStudent" table="ClassSessionStudent" >
    <id name="Id" type="Int32">
      <column name="Id"  />
      <generator class="assigned" />
    </id>

    <many-to-one name="MyClassSession" class="ClassSession">
      <column name="ClassSessionId" not-null="false" precision="10" scale="0" sql-type="int" />
    </many-to-one>

    <many-to-one name="MyStudent" class="Student">
      <column name="StudentId" not-null="false" precision="10" scale="0" sql-type="int" />
    </many-to-one>

  </class>
</hibernate-mapping>







